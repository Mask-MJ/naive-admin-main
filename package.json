{
  "name": "naive-admin-main",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/qs": "^6.9.7",
    "axios": "^1.3.6",
    "axios-retry": "^3.4.0",
    "colord": "^2.9.3",
    "cropperjs": "^1.5.13",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.2",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.3",
    "pinia": "^2.0.35",
    "qs": "^6.11.1",
    "sortablejs": "^1.15.0",
    "vue": "^3.2.47",
    "vue-draggable-plus": "^0.1.6",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@iconify-json/ant-design": "^1.1.5",
    "@iconify-json/carbon": "^1.1.16",
    "@iconify-json/line-md": "^1.1.24",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/sortablejs": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@unocss/transformer-directives": "^0.51.8",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vueuse/core": "^10.1.0",
    "@vueuse/head": "^1.1.26",
    "commitizen": "^4.3.0",
    "cz-git": "^1.6.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "msw": "^1.2.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "typescript": "^5.0.2",
    "unocss": "^0.51.8",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.0",
    "vite-plugin-inspect": "^0.7.24",
    "vite-plugin-mkcert": "^1.14.0",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(npm)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{scss,less,styl,html}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
